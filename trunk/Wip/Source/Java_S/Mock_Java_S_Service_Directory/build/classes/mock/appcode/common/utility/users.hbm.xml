<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="true" default-lazy="false">
	<class name="mock.appcode.common.utility.Users" table="User2"  catalog="Service_Directory">
		<id name="userid" type="string">
			<column name="UserID" length="15" not-null="true" />
			<generator class="identity" />
		</id>

		<property name="account" type="string">
			<column name="Account" length="15" not-null="true" />
		</property>

		<property name="email" type="string">
			<column name="Email" length="50" not-null="true" />
		</property>

		<property name="roles" type="string">
			<column name="Roles" length="15" not-null="true" />
		</property>

		<property name="password" type="string">
			<column name="Password" length="15" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
